# ==============================================
# AURALIS TRANSCRIPTOR - FRONTEND ENV (PRODUCTION)
# ==============================================

# These variables are read at build time by Vite.
# On Vercel, set them in Project Settings > Environment Variables.

# Supabase (Required)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# API base URL served by your backend deployment
# Example: https://api.yourdomain.com/api
VITE_API_URL=

# App metadata
VITE_APP_NAME=Aurora Transcriptor
VITE_APP_VERSION=

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SENTRY=false
